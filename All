âœ… 20. WAP to check vowel or consonant

#include <stdio.h>
int main() {
    char ch;
    scanf("%c", &ch);

    if (ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||
        ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U')
        printf("Vowel");
    else
        printf("Consonant");

    return 0;
}


---

âœ… 21. WAP to check Armstrong number

#include <stdio.h>
int main() {
    int n, temp, rem, sum = 0;
    scanf("%d", &n);
    temp = n;

    while (temp > 0) {
        rem = temp % 10;
        sum += rem * rem * rem;
        temp /= 10;
    }

    if (sum == n)
        printf("Armstrong");
    else
        printf("Not Armstrong");
    return 0;
}


---

âœ… 22. WAP to display Armstrong numbers between 1 to 1000

#include <stdio.h>
int main() {
    int i, temp, rem, sum;
    for (i = 1; i <= 1000; i++) {
        temp = i;
        sum = 0;
        while (temp > 0) {
            rem = temp % 10;
            sum += rem * rem * rem;
            temp /= 10;
        }
        if (sum == i)
            printf("%d ", i);
    }
    return 0;
}


